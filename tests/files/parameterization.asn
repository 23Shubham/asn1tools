Parameterization DEFINITIONS AUTOMATIC TAGS ::=

BEGIN

A { B } ::= SEQUENCE {
    a B
}

A-Boolean ::= A { BOOLEAN }

A-Integer ::= A { INTEGER }

B { A, B } ::= SEQUENCE {
    a A,
    b B OPTIONAL
}

B-BooleanInteger ::= B { BOOLEAN, INTEGER }

C { A } ::= CHOICE {
    a A,
    b B { A, INTEGER }
}

D ::= SEQUENCE {
    a C { B-BooleanInteger },
    b CHOICE {
        c A { B { NULL, INTEGER } },
        d B { NULL, INTEGER }
    }
}

E { A } ::= SEQUENCE (SIZE (0..5)) OF CHOICE {
    a A,
    b SEQUENCE {
        c SEQUENCE OF B { A, BOOLEAN }
    }
}

F ::= E { INTEGER }

END
